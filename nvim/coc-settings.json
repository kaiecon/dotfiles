{
  "suggest.timeout": 5000,
  "diagnostic.displayByAle": true,
  "languageserver": {
    "clangd": {
      "command": "clangd",
      "rootPatterns": [
        "compile_flags.txt",
        "compile_commands.json",
        ".vim/",
        ".git/",
        ".hg/"
      ],
      "filetypes": ["c", "cpp", "objc", "objcpp"]
    }
  },
  "python.jediEnabled": false,
  "vetur.trace.server": "verbose",
  "tsserver.enable": false
}
